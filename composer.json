{
    "name": "anime-db/shikimori-filler-bundle",
    "license": "GPL-3.0",
    "type": "anime-db-plugin",
    "description": "Plugin Shikimori for the catalog Anime DB",
    "homepage": "http://github.com/anime-db/shikimori-filler-bundle",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\ShikimoriFillerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Bundle\\ShikimoriFillerBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "anime-db/shikimori-browser-bundle": "1.0.*"
    },
    "require-dev": {
        "anime-db/catalog-bundle": ">=0.4.17",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/form": "~2.4|~3.0",
        "phpunit/phpunit": "^4.8.0",
        "scrutinizer/ocular": "~1.3.0",
        "satooshi/php-coveralls": "^1.0.0"
    },
    "minimum-stability": "dev"
}
